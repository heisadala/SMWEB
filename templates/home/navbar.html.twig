            <div id="sm-body-home-navbar-navbar-collapse" class="navbar-collapse collapse">
                <ul id="sm-body-home-navbar-navbar-collapse-ul" class="navbar-nav">
                    <li id="sm-body-home-navbar-navbar-collapse-item" class="nav-item dropdown">
                        <a id="sm-body-home-navbar-nav-link" 
                            class="nav-link dropdown-toggle" 
                            data-bs-toggle="dropdown"  
                            role="button" 
                            aria-expanded="false">
                            <img id='sm-body-home-navbar-nav-link-img' 
                                class='img-fluid' 
                                src="{{ server_base }}/images/icon/Database.png">
